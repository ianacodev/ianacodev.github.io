{
  "name": "about-me",
  "version": "0.0.0",
  "scripts": {
    "deploy": "git add --all && git commit -m 'auto-update' && git push -u origin main",
    "bundle:analyze": "webpack-bundle-analyzer docs/stats.json",
    "build:move": "move-cli ./temp/404.html ./docs/404.html && rmdir temp",
    "build:copy": "cpy ./docs/index.html ./temp --flat --rename=404.html && npm run build:move",
    "build:git": "ng build --output-path=docs --stats-json --configuration production && npm run build:copy",
    "ng": "ng",
    "start": "ng serve --host 192.168.1.56",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.0",
    "@angular/cdk": "^13.3.2",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/flex-layout": "^13.0.0-beta.38",
    "@angular/forms": "~13.2.0",
    "@angular/material": "^13.3.2",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/router": "~13.2.0",
    "@ngrx/entity": "^13.1.0",
    "@ngrx/router-store": "^13.1.0",
    "@ngrx/store": "^13.1.0",
    "@ngrx/store-devtools": "^13.1.0",
    "hamburgers": "^1.2.1",
    "ngx-drag-scroll": "^13.0.0-beta.2",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.5",
    "@angular/cli": "~13.2.5",
    "@angular/compiler-cli": "~13.2.0",
    "@types/d3": "^7.1.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "cpy-cli": "^4.1.0",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "move-cli": "^2.0.0",
    "typescript": "~4.5.2",
    "webpack-bundle-analyzer": "^4.5.0"
  }
}
